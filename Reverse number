import java.util.*;
class Main {
    public static void main(String[] args) {
        int n;
        Scanner sc =new Scanner(System.in);
        System.out.println("Enter the number to be reversed");
        n=sc.nextInt();
        int sum=0;
        while(n>0){
            int lastdigit=n%10;
            sum=sum*10+(lastdigit);
            n=n/10;
        }
        System.out.println(sum);
    }
}
/*OUTPUT:
Enter the number to be reversed
139246
642931
*/
